<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"	
	default-activation="lazy">
	<service id="ftpDescriptorService" ref="ftpDescriptor" auto-export="interfaces">
		<service-properties>
			<entry key="name" value="org.backmeup.ftp"/>
		</service-properties>
	</service>
	<service id="ftpAuthService" ref="ftpAuthenticator" auto-export="interfaces">
		<service-properties>
			<entry key="name" value="org.backmeup.ftp"/>
		</service-properties>
	</service>
	<service id="ftpDatasourceService" ref="ftpDatasource" auto-export="interfaces">
		<service-properties>
			<entry key="name" value="org.backmeup.ftp"/>
		</service-properties>
	</service>
</blueprint>